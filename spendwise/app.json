{
  "expo": {
    "name": "spendwise",
    "slug": "spendwise",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.tasluf.spendwise",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tasluf.spendwise"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tasluf.spendwise"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4af2d432-4df6-4f54-b185-a394e0b51f17"
      }
    },
    "owner": "tasluf"
  }
}
