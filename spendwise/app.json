{
  "expo": {
    "name": "spendwise",
    "slug": "spendwise",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.tasluf.spendwise",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tasluf.spendwise"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tasluf.spendwise",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "com.tasluf.spendwise"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Funnel_Display/FunnelDisplay_Bold.ttf",
            "./assets/fonts/Funnel_Display/FunnelDisplay_Light.ttf",
            "./assets/fonts/Funnel_Display/FunnelDisplay_ExtraBold.ttf",
            "./assets/fonts/Funnel_Display/FunnelDisplay_Medium.ttf",
            "./assets/fonts/Funnel_Display/FunnelDisplay_Regular.ttf",
            "./assets/fonts/Funnel_Display/FunnelDisplay_SemiBold.ttf",
            "./assets/fonts/Funnel_Display/FunnelDisplay_VariableFont_wght.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4af2d432-4df6-4f54-b185-a394e0b51f17"
      }
    },
    "owner": "tasluf"
  }
}
